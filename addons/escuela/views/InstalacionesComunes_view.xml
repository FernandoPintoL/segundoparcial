<odoo>
  <data>
    <record model="ir.actions.act_window" id="escuela.instalacionescomunes_action_window">
      <field name="name">Instalaciones Comunes</field>
      <field name="res_model">escuela.instalacionescomunes</field>
      <field name="view_mode">tree,form</field>
    </record>

        <!-- explicit list view definition -->

    <record id="escuela.instalacionescomunes_list" model="ir.ui.view">
      <field name="name">instalacionescomunes.view.tree</field>
      <field name="model">escuela.instalacionescomunes</field>
      <field name="arch" type="xml">
        <tree>
          <field name="name"></field>
          <field name="capacidad"></field>
          <field name="colegio_id"></field>
        </tree>
      </field>
    </record>

    <record id="escuela.instalacionescomunes_form" model="ir.ui.view">
      <field name="name">instalacionescomunes.view.form</field>
      <field name="model">escuela.instalacionescomunes</field>
      <field name="arch" type="xml">
        <form>
          <sheet>
              <group>
                <field name="name"></field>
                <field name="capacidad"></field>
                <field name="colegio_id"></field>
              </group>
          </sheet>
        </form>
      </field>
    </record>

