<odoo>
  <data>
    <record model="ir.actions.act_window" id="escuela.gestionacademica_action_window">
      <field name="name">Gestion Academica</field>
      <field name="res_model">escuela.gestionacademica</field>
      <field name="view_mode">tree,form</field>
    </record>

    <record id="escuela.gestionacademica_list" model="ir.ui.view">
      <field name="name">gestionacademica.view.tree</field>
      <field name="model">escuela.gestionacademica</field>
      <field name="arch" type="xml">
        <tree>
          <field name="name"></field>
          <field name="gestionPeriodo"></field>
          <field name="fechaInicial"></field>
          <field name="fechaFinal"></field>
          <field name="modoEvaluacion"></field>
          <field name="cicloescolaridad_ids"></field>
          <field name="mensualidad_ids"></field>
        </tree>
      </field>
    </record>

    <record id="escuela.gestionacademica_form" model="ir.ui.view">
      <field name="name">gestionacademica.view.form</field>
      <field name="model">escuela.gestionacademica</field>
      <field name="arch" type="xml">
        <form>
          <sheet>
              <group>
                <field name="name"></field>
                <field name="gestionPeriodo"></field>
                <field name="fechaInicial"></field>
                <field name="fechaFinal"></field>
                <field name="modoEvaluacion"></field>
                <field name="cicloescolaridad_ids"></field>
                <field name="mensualidad_ids"></field>
              </group>
          </sheet>
        </form>
      </field>
    </record>

